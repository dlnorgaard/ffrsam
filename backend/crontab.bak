# 10 minute RSAM
*/10 * * * * /opt/ffrsam/run.sh 10 > /opt/ffrsam/log/rsam10.log 2>&1
# 1 hour RSAM
20 * * * * /opt/ffrsam/run.sh 60 > /opt/ffrsam/log/rsam60.log 2>&1
# 4 hour RSAM
20 */4 * * * /opt/ffrsam/run.sh 240 > /opt/ffrsam/log/rsam240.log 2>&1
# 12 hour RSAM
20 */12 * * * /opt/ffrsam/run.sh 720 > /opt/ffrsam/log/rsam720.log 2>&1
# 1 day RSAM
20 0 * * * /opt/ffrsam/run.sh 1440 > /opt/ffrsam/log/rsam1440.log 2>&1
